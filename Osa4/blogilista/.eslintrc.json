{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 15
    },
    "rules": {
        "no-console":0,
        "no-underscore-dangle":0,
        "linebreak-style":0,
        "semi":0,
        "packageDir":0,
        "import/no-extraneous-dependencies": 0,
        "no-param-reassign":0
    }
}
